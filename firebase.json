"hosting": {
  "public": "public",
  "rewrites": [
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/**/*.@(html|css|js|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600, stale-while-revalidate=3600"
        }
      ]
    }
  ]
}
